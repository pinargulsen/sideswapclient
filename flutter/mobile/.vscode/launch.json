    "version": "0.2.0",
    "configurations": [
        {
            "name": "mobile (debug mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_mobile.dart",
        },
        {
            "name": "mobile (desktop debug mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_mobile.dart",
            "args": [
                "--dart-entrypoint-args", "--mode=mobile"
            ]
        },
        {
            "name": "mobile (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
        },
        {
            "name": "mobile (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
        }
    ]
}